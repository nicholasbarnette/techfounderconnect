{
 "name": "techfounderconnect",
 "version": "0.0.1",
 "description": "The landing page of Tech Founder Connect.",
 "main": "index.js",
 "scripts": {
  "start": "webpack-dev-server --config webpack.config.js --open",
  "build": "webpack --config webpack.config.js --progress",
  "format": "npm run format:prettier && npm run format:tslint",
  "format:prettier": "prettier --write ./src",
  "format:tslint": "tslint -p tsconfig.json -c tslint.json ./src/**/*.{ts,tsx,js,jsx} --fix"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/nicholasbarnette/techfounderconnect.git"
 },
 "author": "Nick Barnette",
 "license": "ISC",
 "bugs": {
  "url": "https://github.com/nicholasbarnette/techfounderconnect/issues"
 },
 "homepage": "https://github.com/nicholasbarnette/techfounderconnect#readme",
 "devDependencies": {
  "@babel/core": "^7.10.2",
  "@babel/preset-env": "^7.10.2",
  "@babel/preset-react": "^7.10.1",
  "@babel/preset-typescript": "^7.10.1",
  "babel-loader": "^8.2.2",
  "copy-webpack-plugin": "^6.4.0",
  "core-js": "^3.8.1",
  "css-loader": "^5.0.1",
  "html-webpack-plugin": "^4.5.0",
  "husky": "^4.3.5",
  "lint-staged": "^10.5.3",
  "mini-css-extract-plugin": "^1.3.2",
  "node-sass": "^5.0.0",
  "prettier": "^2.2.1",
  "sass-loader": "^10.1.0",
  "style-loader": "^2.0.0",
  "ts-loader": "^8.0.11",
  "ts-node": "^9.1.1",
  "tslint": "^6.1.3",
  "tslint-immutable": "^6.0.1",
  "typescript": "^4.1.2",
  "webpack": "^4.43.0",
  "webpack-cli": "^3.3.11",
  "webpack-dev-server": "^3.11.0"
 },
 "lint-staged": {
  "./src/**/*.{ts,tsx,js,jsx}": [
   "tslint -p tsconfig.json -c tslint.json --fix",
   "prettier --write"
  ],
  "./src/**/*.css": [
   "prettier --write"
  ]
 },
 "husky": {
  "hooks": {
   "pre-commit": "lint-staged"
  }
 },
 "dependencies": {}
}
